                                    <li>
                                        <a href="~/Caracteristicas/Index"><i class="fa fa-check fa-fw" style="color:#f0ad4e"></i>Caracteristicas</a>
                                    </li>
                                }
                                @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Activo_Puede_Acceder_Item_Catalogo_Activos))
                                {
                                    <li>
                                        <a href="~/CatalogoProducto/Index"><i class="fa fa-check fa-fw" style="color:#f0ad4e"></i>Catalogo de Activos</a>
                                    </li>
                                }
                                @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Activo_Puede_Acceder_Item_Tipo_Activos))
                                {
                                    <li>
                                        <a href="~/TipoProducto/Index"><i class="fa fa-check fa-fw" style="color:#f0ad4e"></i>Tipo de Activos</a>
                                    </li>
                                }
                                @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Activo_Puede_Acceder_Item_Activos))
                                {
                                    <li>
                                        <a href="~/Producto/Index"><i class="fa fa-product-hunt fa-fw" style="color:#f0ad4e"></i>Activos</a>
                                    </li>
                                }

                                </ul>
