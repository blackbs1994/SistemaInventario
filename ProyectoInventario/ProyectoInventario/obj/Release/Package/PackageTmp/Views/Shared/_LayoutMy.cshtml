                        <li>
                            <a href="#"><i class="fa fa-car fa-fw"></i> Activos<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Activo_Puede_Acceder_Item_Marca))
                                {
                                    <li>
                                        <a href="~/Marca/Index"> <i class="fa fa-check fa-fw" style="color:#f0ad4e"></i>Marca</a>
                                    </li>
                                }
                                @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Activo_Puede_Acceder_Item_Modelo))
                                {
                                    <li>
                                        <a href="~/Modelo/Index"> <i class="fa fa-check fa-fw" style="color:#f0ad4e"></i>Modelo</a>
                                    </li>
                                }
