                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">

                        <li>
                            <a href="~/Inicio/Index"><i class="fa fa-home fa-fw"></i> Principal</a>
                        </li>
                        
                        <li>
                            <a href="#"><i class="fa fa-wrench fa-fw"></i> Administración<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                    @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Administracion_Puede_Acceder_Item_Usuarios))
                                    {
                                        <li>
                                            <a href="~/Usuario/Index"><i class="fa fa-user fa-fw" style="color:#f0ad4e"></i> Usuarios</a>
                                        </li>
                                    }
                                    @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Administracion_Puede_Acceder_Item_Custodios))
                                    {

                                        <li>
                                            <a href="~/Usuario/IndexCustodio"><i class="fa fa-user fa-fw" style="color:#f0ad4e"></i> Custodios</a>
                                        </li>
                                    }

                                    @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Administracion_Puede_Acceder_Item_Tipo_Departamento))
                                    {
                                        <li>
                                            <a href="~/TipoDepartamento/Index"><i class="fa fa-building-o fa-fw" style="color:#f0ad4e"></i>Tipo Departamentos</a>
                                        </li>
                                    }
                                    @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Administracion_Puede_Acceder_Item_Departamento))
                                    {
                                        <li>
                                            <a href="~/Departamento/Index">Departamentos</a>
                                        </li>
                                    }
                                    @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Administracion_Puede_Acceder_Item_Ubicaciones))
                                    {
                                        <li>
                                            <a href="~/Ubicacion/Index">Ubicaciones</a>
                                        </li>
                                    }
                                </ul>
                        </li>
