                                        <a href="~/TipoCaracteristicas/Index"><i class="fa fa-check fa-fw" style="color:#f0ad4e"></i>Tipo Caracteristica</a>
                                    </li>
                                }
                                @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Activo_Puede_Acceder_Item_Caracteristica))
