                                {
                                    <li>
                                        <a href="~/Acta/GenerarActa"> <i class="fa fa-file-archive-o fa-fw" style="color:#f0ad4e"></i>Generar Acta Entrega</a>
                                    </li>
                                }
                                @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Actas_Puede_Acceder_Item_Acta_Descargo))
                                {
                                    <li>
                                        <a href="~/BienesPorCustodio/IndexBienesPorCustodio"><i class="fa fa-file-archive-o fa-fw" style="color:#f0ad4e"></i>Generar Acta de Descargo</a>
                                    </li>
                                }
                                @if (AccesoDatos.Commons.FrontUser.TienePermiso(AccesoDatos.Commons.RolesPermisos.Actas_Puede_Acceder_Item_Consulta))
